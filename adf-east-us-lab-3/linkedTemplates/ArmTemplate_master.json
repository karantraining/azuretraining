{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-east-us-lab-3"
		},
		"rawblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'rawblob'"
		},
		"HttpServer1lab3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.reddit.com/r/sports/comments/.json?sort=new&limit=100"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-east-us-lab-3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"rawblob_connectionString": {
						"value": "[parameters('rawblob_connectionString')]"
					},
					"HttpServer1lab3_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1lab3_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}